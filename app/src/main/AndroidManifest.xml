<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uniquitousprototype">

    <application android:name=".ApiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InitiatePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".LoginPage">

        </activity>
        <activity android:name=".MainActivity">
        </activity>

        <activity android:name=".CreateTaskIntent"
            android:label="새로운 일을 추가합니다."
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>

        </activity>
        <activity android:name=".StartNegotiationIntent"
            android:label="협상을 진행하시겠습니까?"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>